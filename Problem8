#include <iostream>

using namespace std;

int main ()
{
    int num, i;
    
    cout << "Enter a number: ";
    cin >> num;
    
    cout << "Multiplication table of " << num << ":" << endl;
    

    for (i=1; i<=10; ++i)
        cout << i << " * " << num << " = " << i*num << endl;
    
    return 0;
}
