#include <iostream>
#include <cmath>

using namespace std;

int main ()
{
    int num1=0, num2=1, num3, counter, terms=20;
    
    cout << "Fibonacci numbers:" << endl;
    cout << num1 << "," << num2 << ",";
    
    for (counter=1; counter<=terms; ++counter)
    {
        num3 = num1 + num2;
        cout << num3 << ",";
        num1 = num2;
        num2 = num3;
    }
    
    return 0;
}
